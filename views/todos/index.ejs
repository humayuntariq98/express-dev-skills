<%- include('../partials/header') %>
    <h3>To-Dos as of <%= locals.time %></h3>
        <ul>
          <% todos.forEach(function(t) { %>
            <li>
                
                <a href="/todos/<%= t.id %>"><%= t.todo %></a>
                - 
              <%= t.done ? 'done' : 'not done' %>
            </li>
          <% }); %>
        </ul> 
        <a href="/todos/new">Add a new To-Do</a>
 <%- include('../partials/footer') %>